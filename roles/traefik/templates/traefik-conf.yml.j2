---
api:
  dashboard: true
  debug: false

serversTransport:
  insecureSkipVerify: true

entryPoints:
  web:
    address: ":80"
    http:
      redirections:
        entryPoint:
          to: websecure
          scheme: https
          permanent: true
  websecure:
    address: ":443"
    forwardedHeaders:
      trustedIPs:
        - "127.0.0.1/32"
        - "192.168.1.1"
  rd5-tcp:
    address: ":21115"
  rd6-tcp:
    address: ":21116"
  rd7-tcp:
    address: ":21117"
  rd8-tcp:
    address: ":21118"
  rd9-tcp:
    address: ":21119"
  rd6-udp:
    address: ":21116/udp"

providers:
  docker:
    endpoint: "unix:///var/run/docker.sock"
    exposedByDefault: false
    swarmMode: true
    watch: true
    network: {{traefik_network}}
  file:
    filename: "/config.yml"
    watch: true

certificatesResolvers:
  letsEncrypt:
    acme:
      caServer: https://acme-v02.api.letsencrypt.org/directory
      email: tboucher@live.com
      storage: acme.json
      dnsChallenge:
        provider: godaddy
        delayBeforeCheck: 0
        resolvers:
        - "1.1.1.1:53"
        - "8.8.8.8:53"

pilot:
    token: "0ae604b9-e403-4150-95c9-34fe538588db"

experimental:
  plugins:
    rewritebody:
      modulename: "github.com/traefik/plugin-rewritebody"
      version: "v0.3.1"
    traefik-themepark:
      moduleName: "github.com/packruler/traefik-themepark"
      version: "v1.3.0"
